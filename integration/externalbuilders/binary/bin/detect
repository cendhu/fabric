# SPDX-License-Identifier: Apache-2.0

set -euo pipefail

if [ "$#" -ne 2 ]; then
    echo "Expected 2 directories got $#"
    exit 2
fi

if [ "$(jq -r .type "$2/metadata.json")" == "binary" ]; then
    exit 0
fi

echo "binary is the only supported type"
exit 1
