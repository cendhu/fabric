set -euo pipefail

DIR="${BASH_SOURCE%/*}"
if [[ ! -d "$DIR" ]]; then DIR="$PWD"; fi
. "$DIR/parse.sh"

if [ "$TYPE" = "GOLANG" ] ; then
    exit 0
fi

echo "Supported types is only golang -- got type '$TYPE'"
exit 1
