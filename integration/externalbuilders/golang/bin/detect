set -euo pipefail

if [ "$#" -ne 2 ]; then
    echo "Expected 2 directories got $#"
    exit 2
fi

if grep -q "golang" "$2/metadata.json" ; then
    exit 0
fi

echo "golang is the only supported type"
exit 1
